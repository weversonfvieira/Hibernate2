<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.weverson.hibernate.Endereco" table="endereco">
<id name="end_Id" column="id_end">
<generator class="increment"></generator>
</id>
<property name="cidade" column="cidade"></property>
<property name="estado" column="estado"></property>
<property name="pais" column="pais"></property>


</class>
</hibernate-mapping>